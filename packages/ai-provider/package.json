{
  "name": "@ellmers/ai-provider",
  "type": "module",
  "version": "0.0.1",
  "description": "Ellmers is a tool for building and running DAG pipelines of AI tasks.",
  "scripts": {
    "watch": "concurrently -c 'auto' 'bun:watch-*'",
    "watch-hf-transformers-inline": "bun build --watch --no-clear-screen --sourcemap=external --packages=external --outdir ./dist/hf-transformers ./src/hf-transformers/inline.ts",
    "watch-tf-mediapipe-inline": "bun build --watch --no-clear-screen --sourcemap=external --packages=external --outdir ./dist/tf-mediapipe ./src/tf-mediapipe/inline.ts",
    "watch-types": "tsc --watch --preserveWatchOutput",
    "build": "bun run build-clean && bun run build-hf-transformers && bun run build-tf-mediapipe && bun run build-types",
    "build-clean": "rm -fr dist/* tsconfig.tsbuildinfo",
    "build-hf-transformers": "bun run build-hf-transformers-inline",
    "build-hf-transformers-inline": "bun build --sourcemap=external --packages=external --outdir ./dist/hf-transformers ./src/hf-transformers/inline.ts",
    "build-tf-mediapipe": "bun run build-tf-mediapipe-inline",
    "build-types": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "bun test"
  },
  "exports": {
    "./hf-transformers/inline": {
      "import": "./dist/hf-transformers/inline.js",
      "types": "./dist/hf-transformers/inline.d.ts"
    },
    "./tf-mediapipe/inline": {
      "import": "./dist/tf-mediapipe/inline.js",
      "types": "./dist/tf-mediapipe/inline.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ellmers/task-graph": "workspace:*",
    "@ellmers/ai": "workspace:*",
    "@ellmers/job-queue": "workspace:*",
    "@ellmers/storage": "workspace:*"
  }
}
